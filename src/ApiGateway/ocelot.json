{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/stock/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "stockdataservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/stock/{everything}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/analysis/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "aianalysis",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/analysis/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigateway:8080"
  }
}