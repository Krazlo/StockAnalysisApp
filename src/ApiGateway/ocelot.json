{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/stock/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "stockdataservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/stock/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1m",
        "Limit": 5
      }
    },
    {
      "DownstreamPathTemplate": "/api/analysis/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "aianalysis",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/analysis/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1m",
        "Limit": 5
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigateway:8080",
    "RateLimitOptions": {
      "DisableRateLimitHeaders": false,
      "QuotaExceededMessage": "Rate limit exceeded. Slow down."
    }
  }
}